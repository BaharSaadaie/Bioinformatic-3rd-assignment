$ sed '/Zoo/d' '/cygdrive/c/Users/12698/Desktop/assignment_3/pandas.txt' >> output_No_Zoo.txt
$ grep 'male' pandas.txt | awk '{print $1, $4, $2}' >> output_Sort_Sex  
